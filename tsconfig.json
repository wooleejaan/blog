{
  "extends": "astro/tsconfigs/strict",
  "baseUrl": ".",
  "compilerOptions": {
    "strictNullChecks": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "plugins": [
    {
      "name": "@astrojs/ts-plugin"
    }
  ],
  "paths": {
    "@components/*": [
      "src/components/*"
    ],
    "@layouts/*": [
      "src/layouts/*"
    ],
    "@pages/*": [
      "src/pages/*"
    ]
  }
}